{
	"explorer.file.column.indent.indentLine": true,
	"explorer.file.showHiddenFiles": true,
	"explorer.keyMappingMode": "none",
	"explorer.buffer.showHiddenBuffers": false,
	"explorer.keyMappings.global": {
		"i": "nodePrev",
		"k": "nodeNext",
		"h": "toggleSelection",
		"<tab>": "actionMenu",
		"gl": "expand:recursive",
		"gh": "collapse:recursive",
		"o": [
			"wait",
			"expandable?",
			"open",
			"open"
		],
		"<cr>": [
			"wait",
			"expandable?",
			"cd",
			"open"
		],
		"so": "open:vsplit",
		"a": [
			"wait",
			"expanded?",
			"collapse",
			"expand"
		],
		"O": "open:tab",
		"n": "collapse",
		"u": "gotoParent",
		"yp": "copyFilepath",
		"yn": "copyFilename",
		"yy": "copyFile",
		"dd": "cutFile",
		"pp": "pasteFile",
		"md": "deleteForever",
		"ma": "addFile",
		"A": "addDirectory",
		"mr": "rename",
		"R": "refresh",
		"?": "help",
		"q": "quit",
		"X": "systemExecute",
		"F": "searchRecursive",
		"B": "gotoSource:file",
		"b": "gotoSource:buffer",
		"[[": "sourcePrev",
		"]]": "sourceNext",
		"[d": "diagnosticPrev",
		"]d": "diagnosticNext",
		"[c": "gitPrev",
		"]c": "gitNext",
		"<<": "gitStage",
		">>": "gitUnstage"
	},


	"coc.preferences.formatOnSave": false,

	"python.analysis.diagnosticSeverityOverrides": {
		"reportOptionalMemberAccess": "warning",
		"reportArgumentType": "warning",
		"reportIncompatibleMethodOverride":"warning",
		"reportOptionalSubscript":"warning"
	},
	"clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/18.1.3/clangd_18.1.3/bin/clangd"
}
